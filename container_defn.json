{
  "family": "spring-task-ec2",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::368355641188:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "spring-container",
      "image": "368355641188.dkr.ecr.us-east-1.amazonaws.com/spring-app:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "500",
  "memory": "500"
}
